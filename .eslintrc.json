{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "require-jsdoc": "off"
    },
    "import/no-extraneous-dependencies": ["error", {"includeInternal": true, "includeTypes": true}]
}
