{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "require-jsdoc": "off",
        "import/no-extraneous-dependencies": ["off", {"includeInternal": true, "includeTypes": true}],
        "import/extensions": ["off", "never", { "svg": "always" }],
        "no-console": "off",
        "consistent-return": "off",
        "arrow-parens":"off",
        "arrow-body-style":"off",
        "max-len":"off",
        "no-param-reassign": "off"
    }
    
}
